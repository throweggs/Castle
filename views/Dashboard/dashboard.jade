							a.nav-link.active(href='/dashboard')
								i.far.fa-home
								|     			        Dashboard 

						h6.sidebar-heading.d-flex.justify-content-between.align-items-center.px-3.mt-4.mb-1.text-muted
							span Visitor Forms  
							.far.fa-list-ul

						ul.nav.flex-column
							li.nav-item
								a#visitorLink.nav-link(href='#')
									i.fas.fa-user
									| 									Visitors
							li.nav-item
								a#wbsLink.nav-link(href='#')
									i.fas.fa-user-circle
									|                   Women's Bouldering Social
							li.nav-item
								a#theSessionLink.nav-link(href='#')
									i.fas.fa-user-circle
									|                   The Session
							li.nav-item
								a#nonClimbingChildLink.nav-link(href='#')
									i.far.fa-child
									|                   Non-Climbing Child
							li.nav-item
								a#thamesWaterLink.nav-link(href='#')
									i.far.fa-tint
									|                   Thames thamesWater
							li.nav-item
								a#gardenLink.nav-link(href='#')
									i.far.fa-leaf
									|                   Garden and Mini Plotter
							
						h6.sidebar-heading.d-flex.justify-content-between.align-items-center.px-3.mt-4.mb-1.text-muted
							span Staff Time Sheets
							.far.fa-calendar
						
						ul.nav.flex-column
							li.nav-item
								a#CheckInLink.nav-link(href='#')
									i.fas.fa-users-cog 
									| 									 Check In
							li.nav-item
								a#TeamsLink.nav-link(href='#')
									i.fas.fa-users-cog 
									| 									 Teams
							li.nav-item
								a#StaffLink.nav-link(href='#')
									i.fas.fa-user-edit
									| 									 Staff
							li.nav-item
								a#todayCheckIn.nav-link(href='#')
									i.fas.fa-calendar-day
									|                   Today's Check Ins
							li.nav-item
								a#weekCheckIn.nav-link(href='#')
									i.fas.fa-calendar-week
									|                   This Week's Check Ins
							li.nav-item
								a#departmentCheckIn.nav-link(href='#')
									.fas.fa-calendar
									|                   Department Check Ins
							li.nav-item
								a#customCheckIn.nav-link(href='#')
									i.fas.fa-calendar-check
